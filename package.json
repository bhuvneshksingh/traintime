{
  "name": "sport-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "dev-start": "react-scripts start",
    "start:dev": "react-scripts start",
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "npm run build-css && webpack --mode production",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate-docs": "jsdoc -c jsdoc.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.11",
    "@date-io/luxon": "^1.3.11",
    "@date-io/moment": "^1.3.11",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.33",
    "@material-ui/pickers": "^3.2.7",
    "axios": "^0.21.1",
    "date-fns": "^2.8.1",
    "eslint-plugin-react-hooks": "^2.2.0",
    "font-awesome": "^4.7.0",
    "google-map-react": "^1.1.5",
    "google-maps-react": "^2.0.2",
    "i18next": "19.0.0",
    "i18next-browser-languagedetector": "^4.0.0",
    "i18next-xhr-backend": "^3.2.0",
    "luxon": "^1.21.2",
    "material-design-icons": "^3.0.1",
    "material-ui": "^0.20.2",
    "material-ui-icons": "^1.0.0-beta.36",
    "material-ui-rating": "^4.2.0",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "node": "^12.13.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-avatar-edit": "^0.8.3",
    "react-background-slider": "^1.2.0",
    "react-dom": "^16.11.0",
    "react-google-places-autocomplete": "^1.5.2",
    "react-i18next": "^11.2.2",
    "react-images": "^1.0.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-router-hash-link": "^1.2.1",
    "react-select": "^3.0.8",
    "react-share": "^3.0.1",
    "react-slick": "^0.25.2",
    "react-tap-event-plugin": "^3.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "validator": "12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/runtime": "^7.7.2",
    "@pixi/jsdoc-template": "^2.4.3",
    "css-loader": "^3.2.0",
    "es6-promise": "^4.2.8",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jsdoc": "^3.6.3",
    "lint-staged": "^9.4.2",
    "node-sass-chokidar": "^1.4.0",
    "prettier": "^1.19.1",
    "prettier-stylelint": "^0.4.2",
    "react-scripts": "^3.2.0",
    "react-test-renderer": "^16.11.0",
    "redux-logger": "^3.0.6",
    "style-loader": "1.0.0",
    "tui-jsdoc-template": "^1.2.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "whatwg-fetch": "^3.0.0"
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
